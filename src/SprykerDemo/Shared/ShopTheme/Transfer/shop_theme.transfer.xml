<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="spryker:transfer-01" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ShopTheme">
        <property name="idShopTheme" type="int"/>
        <property name="name" type="string"/>
        <property name="data" type="array"/>
        <property name="status" type="string"/>
        <property name="storeRelation" type="StoreRelation"/>
        <property name="shopThemeData" type="ShopThemeData"/>
    </transfer>

    <transfer name="ShopThemeData">
        <property name="headerColor" type="string"/>
        <property name="headerColorEnabled" type="bool"/>
        <property name="headerTopbarColor" type="string"/>
        <property name="headerTopbarColorEnabled" type="bool"/>
        <property name="headerNavigationBackground" type="string"/>
        <property name="headerNavigationBackgroundEnabled" type="bool"/>
        <property name="headerNavigationTextColor" type="string"/>
        <property name="headerNavigationTextColorEnabled" type="bool"/>
        <property name="footerColor" type="string"/>
        <property name="footerColorEnabled" type="bool"/>
        <property name="footerBottomLineBackground" type="string"/>
        <property name="footerBottomLineBackgroundEnabled" type="bool"/>
        <property name="footerBottomLineTextColor" type="string"/>
        <property name="footerBottomLineTextColorEnabled" type="bool"/>
        <property name="buttonPrimaryColor" type="string"/>
        <property name="buttonPrimaryColorEnabled" type="bool"/>
        <property name="buttonSecondaryColor" type="string"/>
        <property name="buttonSecondaryColorEnabled" type="bool"/>

        <property name="headerTopBackgroundColor" type="string"/>
        <property name="headerTopBackgroundColorEnabled" type="bool"/>
        <property name="headerTopTextColor" type="string"/>
        <property name="headerTopTextColorEnabled" type="bool"/>
        <property name="headerTopIconColor" type="string"/>
        <property name="headerTopIconColorEnabled" type="bool"/>
        <property name="headerMainBackgroundColor" type="string"/>
        <property name="headerMainBackgroundColorEnabled" type="bool"/>
        <property name="headerMainTextColor" type="string"/>
        <property name="headerMainTextColorEnabled" type="bool"/>
        <property name="headerMainIconColor" type="string"/>
        <property name="headerMainIconColorEnabled" type="bool"/>
        <property name="headerBottomBackgroundColor" type="string"/>
        <property name="headerBottomBackgroundColorEnabled" type="bool"/>
        <property name="headerBottomTextColor" type="string"/>
        <property name="headerBottomTextColorEnabled" type="bool"/>
        <property name="headerBottomIconColor" type="string"/>
        <property name="headerBottomIconColorEnabled" type="bool"/>

        <property name="footerNewsletterSectionBackgroundColor" type="string"/>
        <property name="footerNewsletterSectionBackgroundColorEnabled" type="bool"/>
        <property name="footerNewsletterSectionTextColor" type="string"/>
        <property name="footerNewsletterSectionTextColorEnabled" type="bool"/>
        <property name="footerNewsletterSectionIconColor" type="string"/>
        <property name="footerNewsletterSectionIconColorEnabled" type="bool"/>
        <property name="footerLogoSectionBackgroundColor" type="string"/>
        <property name="footerLogoSectionBackgroundColorEnabled" type="bool"/>
        <property name="footerLogoSectionTextColor" type="string"/>
        <property name="footerLogoSectionTextColorEnabled" type="bool"/>
        <property name="footerLogoSectionIconColor" type="string"/>
        <property name="footerLogoSectionIconColorEnabled" type="bool"/>
        <property name="footerNavigationSectionBackgroundColor" type="string"/>
        <property name="footerNavigationSectionBackgroundColorEnabled" type="bool"/>
        <property name="footerNavigationSectionTextColor" type="string"/>
        <property name="footerNavigationSectionTextColorEnabled" type="bool"/>
        <property name="footerNavigationSectionIconColor" type="string"/>
        <property name="footerNavigationSectionIconColorEnabled" type="bool"/>
        <property name="footerCopyrightSectionBackgroundColor" type="string"/>
        <property name="footerCopyrightSectionBackgroundColorEnabled" type="bool"/>
        <property name="footerCopyrightSectionTextColor" type="string"/>
        <property name="footerCopyrightSectionTextColorEnabled" type="bool"/>
        <property name="footerCopyrightSectionIconColor" type="string"/>
        <property name="footerCopyrightSectionIconColorEnabled" type="bool"/>

        <property name="backofficeThemeColor" type="string"/>
        <property name="backofficeThemeColorEnabled" type="bool"/>
        <property name="mpThemeColor" type="string"/>
        <property name="mpThemeColorEnabled" type="bool"/>
        <property name="mpThemeDarkColor" type="string"/>
        <property name="mpThemeDarkColorEnabled" type="bool"/>
        <property name="mpThemeLightColor" type="string"/>
        <property name="mpThemeLightColorEnabled" type="bool"/>
        <property name="logoUrl" type="string"/>
        <property name="backofficeLogoUrl" type="string"/>
        <property name="mpLogoUrl" type="string"/>
        <property name="logoFile" type="File"/>
        <property name="backofficeLogoFile" type="File"/>
        <property name="mpLogoFile" type="File"/>
        <property name="deleteLogo" type="bool"/>
        <property name="deleteMpLogo" type="bool"/>
        <property name="deleteBackofficeLogo" type="bool"/>
    </transfer>

    <transfer name="ShopThemeCriteria">
        <property name="idShopTheme" type="int"/>
        <property name="shopThemeIds" type="int[]" singular="shopThemeId"/>
        <property name="storeIds" type="int[]" singular="storeId"/>
        <property name="storeName" type="string"/>
        <property name="status" type="string"/>
        <property name="excludedShopThemeIds" type="int[]" singular="excludedShopThemeId"/>
        <property name="withStoreRelations" type="bool"/>
        <property name="shopThemeName" type="string"/>
    </transfer>

    <transfer name="StoreRelation">
        <property name="idStores" type="int[]" singular="idStores"/>
        <property name="idEntity" type="int"/>
        <property name="stores" type="Store[]" singular="stores"/>
    </transfer>

    <transfer name="Store">
        <property name="idStore" type="int"/>
    </transfer>

    <transfer name="ActivateShopThemeActionResponse">
        <property name="isSuccess" type="bool"/>
        <property name="errorMessage" type="string"/>
        <property name="conflictingShopTheme" type="ShopTheme"/>
    </transfer>

    <transfer name="ShopThemeResponse">
        <property name="isSuccess" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>

    <transfer name="File">
        <property name="fileName" type="string"/>
        <property name="fileContent" type="string"/>
    </transfer>
</transfers>
