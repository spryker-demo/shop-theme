{
    "name": "spryker-demo/shop-theme",
    "type": "library",
    "description": "ShopTheme module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker-demo/url-builder": "^1.0.0",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/config": "^3.6.0",
        "spryker/file-system": "^2.0.0",
        "spryker/kernel": "^3.75.0",
        "spryker/log": "^3.0.0",
        "spryker/propel-orm": "^1.17.0",
        "spryker/ramsey-uuid": "^1.0.0 || ^2.0.0",
        "spryker/store": "^1.16.1",
        "spryker/util-encoding": "^2.1.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
